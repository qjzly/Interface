<Ui>

	<!-- the notes/preferences/target buttons that appear on team lists and the floating footnote -->
	<Button name="RematchTeamFootnoteTemplate" virtual="true">
		<Size x="28" y="28"/>
		<NormalTexture file="Interface\Store\category-icon-scroll">
			<TexCoords left="0.15" right="0.85" top="0.15" bottom="0.85"/>
		</NormalTexture>
		<PushedTexture file="Interface\Store\category-icon-scroll">
			<TexCoords left="0.1" right="0.9" top="0.1" bottom="0.9"/>
		</PushedTexture>
		<HighlightTexture file="Interface\Store\minimap-delivery-highlight" blend="ADD" alpha="0.5">
			<Size x="26" y="26"/>
			<Anchors>
				<Anchor point="CENTER"/>
			</Anchors>
		</HighlightTexture>
		<Scripts>
			<OnLoad>
				if self.defaultIcon then
					self:SetNormalTexture(self.defaultIcon)
					self:SetPushedTexture(self.defaultIcon)
				end
			</OnLoad>
			<OnEnter>
				Rematch.TeamFootnoteOnEnter(self)
			</OnEnter>
			<OnLeave>
				Rematch.TeamFootnoteOnLeave(self)
			</OnLeave>
			<OnClick>
				Rematch.TeamFootnoteOnClick(self)
			</OnClick>
		</Scripts>
	</Button>

